# Solana RPC URL (use a paid RPC for production - Helius, QuickNode, etc.)
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# Solana WebSocket URL (for real-time monitoring)
SOLANA_WS_URL=wss://api.mainnet-beta.solana.com

# Telegram Bot Token (get from @BotFather)
TELEGRAM_BOT_TOKEN=

# Telegram Chat ID (your user ID or group ID for alerts)
TELEGRAM_CHAT_ID=

# RugCheck API Key (optional - get from rugcheck.xyz)
RUGCHECK_API_KEY=

# Minimum liquidity in USD to trigger alert
MIN_LIQUIDITY_USD=1000

# Minimum risk score to send alert (0-100, lower = riskier)
MIN_RISK_SCORE=0

# Monitoring intervals (milliseconds)
JUPITER_POLL_INTERVAL=30000
PUMPFUN_POLL_INTERVAL=10000

# Rate limiting
MAX_REQUESTS_PER_MINUTE=60
